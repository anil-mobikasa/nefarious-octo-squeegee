<h1>SUPER ADMIN DASHBOARD</h1>
<h1>Logged In as: <%= current_user.email %></h1>
<h1>Listing Dietitians</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <!-- <th>Role</th> -->
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dietitians.each do |dietitian| %>
      <tr>
        <td><%= dietitian.name %></td>
        <td><%= dietitian.email %></td>
        <td><%#= dietitian.role.name %></td>
        <td><%#= link_to 'Show', dietitian %></td>
        <td><%#= link_to 'Edit', edit_user_path(dietitian) %></td>
        <td><%#= link_to 'Destroy', dietitian, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

